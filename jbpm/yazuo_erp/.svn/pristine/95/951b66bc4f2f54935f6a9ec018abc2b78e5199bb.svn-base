.module-pptplayer .module-ppt-player {
    width: 798px;
    height: 598px;
    border: 1px solid #e4e4e4;
    background: #ffffff;
    overflow: hidden;
}
.module-pptplayer .module-ppt-player img {
    width: 100%;
}
.module-pptplayer .module-ppt-nav-wrapper {
    width: 148px;
    border: 1px solid #e4e4e4;
    margin-left: 24px;
    background: #ffffff;
}
.module-pptplayer .ppt-nav-up,
.module-pptplayer .ppt-nav-down {
    height: 32px;
    line-height: 32px;
    text-align: center;
    cursor: pointer;
}
.module-pptplayer .icon-nav-arrow {
    font-size: 24px;
    color: #fd8712;
}
.module-pptplayer .is-nav-disabled .icon-nav-arrow {
    color: #bcbbbb;
}
.module-pptplayer .ppt-nav-list-wrapper {
    height: 534px;
    overflow: hidden;
    padding: 0 16px;
    position: relative;
}
.module-pptplayer .ppt-nav-list {
    height: 10000px;
    position: absolute;
    top: 0;
    left: 16px;
}
.module-pptplayer .ppt-nav-item {
    width: 116px;
    height: 84px;
    overflow: hidden;
    margin-bottom: 12px;
    position: relative;
    cursor: pointer;
}
.module-pptplayer .ppt-nav-item img {
    width: 100%;
}
.module-pptplayer .overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #000000;
    opacity: 0.5;
    *filter:opacity(50);
}
.module-pptplayer .ppt-nav-item .sup-tip {
    position: absolute;
    top: 0;
    right: 0;
    padding: 0 0.2em;
    height: 20px;
    background: #66ccb5;
    color: #e9e9e9;
    font-size: 14px;
    text-align: center;
}
.module-pptplayer .is-current-item {
    width: 112px;
    height: 80px;
    border:2px solid #fd8712;
}
