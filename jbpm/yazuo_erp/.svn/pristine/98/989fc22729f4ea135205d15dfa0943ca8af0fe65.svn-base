<div class="page-sales-frontend-customer">
<div class="page-tbar">
    <ul class="page-nav-crumbs">
        <li class="nav-item" ms-repeat="navCrumbs">
            <a ms-href="el.href" class="nav-l" ms-if="el.href">{{el.text}}</a>
            <span class="nav-text" ms-if="!el.href">{{el.text}}</span>
            <span class="separater" ms-if="el.href">&nbsp;>&nbsp;</span>
        </li>
    </ul>
</div>
<div class="page-bd page-bd-form page-padding">
<!--client-header-->
<div class="client-head">
    <!--client-left-->
    <div class="client-head-left">
        <div class="client-head-left-logo">
            <img ms-src="{{merchantData.imgUrl}}">
        </div>
        <div class="client-head-left-text pt20">
            <h2>{{merchantData.merchant_name}}</h2>
            <ul>
                <li>启动日期：{{merchantData.begin_time|date("yyyy-MM-dd")}}</li>
                <li>计划上线：{{merchantData.plan_online_time|date("yyyy-MM-dd")}}</li>
            </ul>
        </div>
    </div>
    <!--client-left-end-->
    <!--client-right-->
    <div class="client_head_right">
        <div class="fn-left pt25">
            <button ms-if="promise" ms-click="localHref('#/frontend/home/mycommunication/')" class="simple-btn mr5"><i class="iconfont icon-blue f26">&#xe612; </i>通讯录</button>
            <button ms-if="promise" ms-click="addQuestions" class="simple-btn mr5"><i class="iconfont icon-blue f26">&#xe60f; </i>添加问题</button>
        </div>
    </div>
    <!--client-right-end-->
</div>
<!--client-header-end-->
<!--client-data-->
<div class="client-top fn-clear mb20">
    <ul class="fn-left client-info-left fn-clear">
        <li>销售负责人：{{merchantData.sale_user_name}}</li>
        <li>业态：{{merchantData.format_name}}</li>
        <li>前端负责人：{{merchantData.front_user_name}}</li>
        <li>门店：{{merchantData.store_number}}家</li>
        <li>人均：{{merchantData.person_avg}}元</li>
        <li>桌均：{{merchantData.desk_avg}}元</li>
    </ul>
    <ul class="fn-right client-info-right">
        <li>开通的产品</li>
        <li>
            <p ms-repeat="merchantData.productList">{{el}}</p>
        </li>
    </ul>
</div>
<!--client-data-end-->

<!--client-step-->
<div class="client-step" ms-repeat="stepData">
    <!--1-->
    <div ms-if="el.stepNum == 'research'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='01'"
             ms-class-2="status-bging:el.onlineProcessStatus =='03'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
             <button class="number">{{el.stepId}}</button>
            <div class="content f14">
                <h2 class="mb5">{{el.fesStep.stepName}}<span ms-if="el.planEndTime">({{el.planEndTime}})</span></h2>
                <div class="title" ms-if="!el.mktBrandInterViews">{{el.fesStep.tip}}</div>
                <div class="type-box">
                    <div class="content-list">
                        <div class="page-button status-style"

                           ms-if="el.mktBrandInterViews">
                            <p class="text" ms-click="readInterval">商户访谈单</p>
                            <p class="choice" ms-click="addInterval">
                                <i ms-if="promise && el.onlineProcessStatus !='04'" class="iconfont">&#xe60f;</i>
                            </p>
                        </div>
                        <div ms-repeat="el.mktShopSurveys" class="page-button status-style">
                            <p class="text"
                               ms-data-id="el.id"
                               ms-click="readSurvey">{{el.storeName}}</p>
                            <p class="choice">
                                <i ms-if="promise && $outer.el.onlineProcessStatus!='04'"
                                   class="iconfont"
                                   ms-click="removeResearch(el.id,el.storeId)" >&#xe616;</i>
                                <i ms-if="promise && $outer.el.onlineProcessStatus!='04'"
                                   class="iconfont"
                                   ms-data-id="el.id"
                                   ms-click="addSurvey">&#xe60f;</i>
                            </p>
                        </div>
                    </div>
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <p class="fn-left-time">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.description}}</p>
                    </div>
                </div>
            </div>
            <div class="operation">
                <p ms-if="promise && el.onlineProcessStatus != '04'">
                <span>
                <button class="simple-btn"
                        ms-click="addSurvey"><i class="iconfont icon-blue f26">&#xe610;</i> 添加调研单</button>
                    </span>
                </p>
                <p ms-if="promise && el.onlineProcessStatus == '03'">
                <span>
                <button class="simple-btn"
                        ms-click="stepEnd(el.id,'04','你确定已经完成了吗？')"><i class="iconfont icon-blue f26">&#xe610;</i> 确认完成</button>
                    </span>
                </p>
                <p ms-if="el.onlineProcessStatus == '04'">
                <span>
                <button class="simple-btn-end"><i class="iconfont icon-blue f26">&#xe610;</i> 已完成</button>
                    </span>
                </p>
            </div>
        </div>
    </div>
    <!--1-end-->
    <!--2-->
    <div ms-if="el.stepNum == 'micro_message'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='02'"
             ms-class-2="status-bging:el.onlineProcessStatus =='03'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
            <button class="number">{{el.stepId}}</button>
            <div class="content f14">
                <h2 class="mb5">{{el.fesStep.stepName}}</h2>
                <div class="title" ms-if="!el.fesRemarks.length">{{el.fesStep.tip}}</div>
                <div class="type-box" ms-if="el.fesRemarks.length">
                    <div class="update-time status-style" ms-repeat-el="el.fesRemarks">
                        <p class="fn-left-time">{{el.updateTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.remark}}</p>
                    </div>
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <p class="fn-left-time">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.description}}</p>
                    </div>
                </div>
            </div>
            <div class="operation">
                <p ms-if="el.onlineProcessStatus == '04'">
                <span>
                <button class="simple-btn-end"><i class="iconfont icon-blue f26">&#xe610;</i> 已完成</button>
                </span>
                </p>
            </div>
        </div>
    </div>
    <!--2-end-->
    <!--3-->
    <div ms-if="el.stepNum == 'draft_program'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='02'"
             ms-class-2="status-bging:el.onlineProcessStatus =='03'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
            <button class="number">{{el.stepId}}</button>
            <div class="content f14 step3-upfile">
                <h2 class="mb5">{{el.fesStep.stepName}}<span ms-if="el.planEndTime"> (预计在{{el.planEndTime| date("yyyy-MM-dd")}}前完成方案洽谈)</span></h2>
                <div class="title" ms-if="!el.fesSysAttachments.length">{{el.fesStep.tip}}</div>
                <div class="type-box" ms-if="el.fesSysAttachments.length">
                    <div class="update-time status-style" ms-repeat-el="el.fesSysAttachments">
                        <p class="fn-left-time">{{el.updateTime|date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">上传了洽谈方案 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>,<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">下载{{el.originalFileName}}</a></p>
                        <p class="iconfont" ms-if="promise && $outer.el.onlineProcessStatus != '04'" ms-click="removeUpload($outer.el.id,el.id,$index)">&#xe616;</p>
                    </div>
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <p class="fn-left-time">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.description}}</p>
                    </div>
                </div>
            </div>
            <div class="operation">
                <p ms-if="promise && el.onlineProcessStatus != '04'">
                <span ms-widget="uploader,$,$step3AttachUploaderOpts">
                <button class="simple-btn"><i class="iconfont icon-blue f26">&#xe611;</i> 会员营销方案</button>
                </span>
                </p>
                <p ms-if="promise && el.onlineProcessStatus == '03'">
                <span>
                <button class="simple-btn"
                        ms-click="stepEnd(el.id,'04','你确定已经完成了吗？')"><i class="iconfont icon-blue f26">&#xe610;</i> 确认完成</button>
                </span>
                </p>
                <p ms-if="el.onlineProcessStatus == '04'">
                <span>
                <button class="simple-btn-end"><i class="iconfont icon-blue f26">&#xe610;</i> 已完成</button>
                </span>
                </p>
            </div>
        </div>
    </div>
    <!--3-end-->
    <!--4-->
    <div ms-if="el.stepNum == 'final_program'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='02'"
             ms-class-2="status-bging:el.onlineProcessStatus =='05'||el.onlineProcessStatus =='06'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
            <button class="number">{{el.stepId}}</button>
            <div class="content f14  step4-upfile">
                <h2 class="mb5">{{el.fesStep.stepName}}<span ms-if="el.planEndTime">({{el.planEndTime}})</span></h2>
                <div class="title" ms-if="!el.fesSysAttachments.length">{{el.fesStep.tip}}</div>
                <div class="type-box">
                    <div class="update-time status-style" ms-repeat-el="el.fesSysAttachments">
                        <p class="fn-left-time">{{el.updateTime|date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">会员营销方案终版 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>,<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">下载{{el.originalFileName}}</a></p>
                        <p class="iconfont" ms-if="promise && $outer.el.onlineProcessStatus!='04'&&$outer.el.onlineProcessStatus!='06'"
                                              ms-click="removeUpload($outer.el.id,el.id,$index)">&#xe616;</p>
                    </div>
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <p class="fn-left-time">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.description}}</p>
                    </div>
                </div>
            </div>
            <div class="operation">
                <p ms-if="promise  && (el.onlineProcessStatus == '02' || el.onlineProcessStatus == '05') && stepData[2].onlineProcessStatus == '04'">
                <span ms-widget="uploader,$,$step4AttachUploaderOpts">
                <button class="simple-btn"><i class="iconfont icon-blue f26">&#xe611;</i> 会员营销方案</button>
                </span>
                </p>
                <p ms-if="promise && el.onlineProcessStatus == '05'">
                <span ms-click="stepEnd(el.id,'06','你确定已经完成了吗？')">
                <button class="simple-btn"><i class="iconfont icon-blue f26">&#xe610;</i> 确认完成</button>
                </span>
                </p>
                <p ms-if="el.onlineProcessStatus == '04'">
                <span>
                <button class="simple-btn-end"><i class="iconfont icon-blue f26">&#xe610;</i> 已完成</button>
                </span>
                </p>
            </div>
        </div>
    </div>
    <!--4-end-->
    <!--5-->
    <div ms-if="el.stepNum == 'background_set'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='02'"
             ms-class-2="status-bging:el.onlineProcessStatus =='03'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
            <button class="number">{{el.stepId}}</button>
            <div class="content f14">
                <h2 class="mb5">{{el.fesStep.stepName}}</h2>
                <div class="title" ms-if="!el.listAttachmentAndOperateLog.length">{{el.fesStep.tip}}</div>
                <div class="type-box">
                    <!--div class="content-list">
                        <p class="merchant-fangtan"
                           ms-if="el.tradeCardtypes.length"
                           ms-click="openCardRuleDialog(el.onlineProcessStatus)">会员规则</p>
                        <p class="merchant-fangtan"
                           ms-if="el.tradeMessageTemplates.length"
                           ms-click="openMessageRules(el.onlineProcessStatus)">交易短信规则</p>
                    </div-->
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <p class="fn-left-time">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.description}}</p>
                    </div>
                </div>
            </div>
            <div class="operation">
                <!--
                <p ms-if="promise && !el.tradeMessageTemplates.length">
                <span>
                <button class="simple-btn" ms-click="openMessageRules(el.onlineProcessStatus)"><i class="iconfont icon-blue f26">&#xe60f;</i> 短信规则</button>
                </span>
                </p>
                <p ms-if="promise && !el.tradeCardtypes.length">
                <span>
                <button class="simple-btn" ms-click="openCardRuleDialog(el.onlineProcessStatus)"><i class="iconfont icon-blue f26">&#xe60f;</i> 会员卡规则</button>
                </span>
                </p>
                <p ms-if="promise && el.onlineProcessStatus == '03'">
                <span ms-click="stepEnd(el.id,'04','你确定已经完成了吗？')">
                <button class="simple-btn"><i class="iconfont icon-blue f26">&#xe610;</i> 确认完成</button>
                </span>
                </p>
                -->
                <p ms-if="el.onlineProcessStatus == '04'">
                <span>
                <button class="simple-btn-end" ms-if="el.onlineProcessStatus == '04'"><i class="iconfont icon-blue f26">&#xe610;</i> 已完成</button>
                </span>
                </p>
            </div>
        </div>
    </div>
    <!--5-end-->
    <!--6-->
    <div ms-if="el.stepNum == 'entity_cards'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='07'"
             ms-class-2="status-bging:el.onlineProcessStatus =='08'||el.onlineProcessStatus =='09'||el.onlineProcessStatus =='10'||el.onlineProcessStatus =='11'||el.onlineProcessStatus =='12'||el.onlineProcessStatus =='13'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
            <button class="number">{{el.stepId}}</button>
            <div class="content f14">
                <h2 class="mb5">{{el.fesStep.stepName}}<span ms-if="el.planEndTime">({{el.planEndTime}})</span></h2>
                <div class="title" ms-if="!el.listAttachmentAndOperateLog.length">{{el.fesStep.tip}}</div>
                <div class="type-box">
                    <div class="update-time status-style" ms-repeat="el.fesSysAttachments">
                        <div ms-if="el.processAttachmentType == '3'">
                            <p class="left-title">{{el.updateTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="left-con">上传卡样源文件 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>，<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">点击下载卡样源文件</a></p>
                        </div>
                        <div ms-if="el.processAttachmentType == '4'">
                            <p class="left-title">{{el.updateTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="left-con">上传卡样确认单 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>，<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">点击下载卡样确认单</a></p>
                        </div>
                        <div ms-if="el.processAttachmentType == '12'">
                            <p class="left-title">{{el.updateTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="left-con">上传开卡数据 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>，<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">点击下载开卡数据</a></p>
                        </div>
                    </div>
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <div ms-if="el.fesLogType == '14'">
                            <p class="left-title">{{el.operatingTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="left-con">{{el.description}}，<a href="javascript:;" ms-click="openCard($outer.el.id,1)">点击查看开卡申请单</a></p>
                        </div>
                        <div ms-if="el.fesLogType !='14'">
                            <p class="left-title">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="left-con">{{el.description}}</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="operation">
                <p  ms-if="el.onlineProcessStatus == '04'">
                <span>
                <button class="simple-btn-end"><i class="iconfont icon-blue f26">&#xe610;</i> 已完成</button>
                </span>
                </p>
            </div>

        </div>
    </div>
    <!--6-end-->
    <!--7-->
    <div ms-if="el.stepNum == 'materials_design'">
        <div class="status-box"
             ms-class-1="status-ready:el.onlineProcessStatus =='18'"
             ms-class-2="status-bging:el.onlineProcessStatus =='19'||el.onlineProcessStatus =='14'||el.onlineProcessStatus =='02'"
             ms-class-3="status-end:el.onlineProcessStatus =='04'"
             ms-class-4="status-past:el.planEndTimeStatus">
            <button class="number">{{el.stepId}}</button>
            <div class="content f14 step7-upfile">
                <h2 class="mb5">{{el.fesStep.stepName}}</h2>
                <div class="title" ms-if="!el.fesMaterialRequests.length">{{el.fesStep.tip}}</div>
                <div class="type-box">
                    <div class="fn-clear">
                        <div class="page-button status-style" ms-if="el.fesMaterialRequests.length">
                            <p class="text"
                               ms-click="step7Input(el.fesMaterialRequests[0].id,'read')">设计需求单</p>
                            <p class="choice" ms-if="promise && el.onlineProcessStatus !='04' && el.onlineProcessStatus !='14' && el.onlineProcessStatus !='02'"  ms-click="step7Input(el.fesMaterialRequests[0].id,'edit')">
                                <i class="iconfont">&#xe60f;</i>
                            </p>
                        </div>
                    </div>
                    <div ms-each="el.fesSysAttachments">
                        <div ms-if="el.processAttachmentType == '6'" class="status-style fn-clear update-time">
                            <p class="fn-left-time">{{el.updateTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="fn-left-text">上传物料源文件 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>，<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">点击下载物料源文件</a></p>
                        </div>
                        <div ms-if="el.processAttachmentType == '15'" class="status-style fn-clear update-time">
                            <p class="fn-left-time">{{el.updateTime | date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                            <p class="fn-left-text">上传了设计素材 <span ms-if="el.userName">[操作人: {{el.userName}}]</span>，<a ms-href="{{path}}fesOnlineProcess/download.do?relPath={{$outer.el.fileConfigPath}}{{el.attachmentPath == null?'':el.attachmentPath}}/{{el.attachmentName}}">点击下载物料设计素材</a></p>
                        </div>
                    </div>
                    <div class="update-time status-style" ms-repeat="el.sysOperationLogVOs">
                        <p class="fn-left-time">{{el.operatingTime |date("yyyy-MM-dd&nbsp;&nbsp;HH:mm")}}</p>
                        <p class="fn-left-text">{{el.description}}</p>
                    </div>
                    <ul class="make-ul" ms-each="el.fesSysAttachments">
                        <li ms-if="el.processAttachmentType != '6'&&el.processAttachmentType != '15'">
                            <p class="make-ul-img">
                                <a ms-href="{{path}}{{$outer.el.fileConfigPath}}/{{el.attachmentName}}" target="_blank">
                                    <img ms-src="{{path}}{{$outer.el.fileConfigPath}}/{{el.attachmentName}}">
                                </a>
                            </p>
                            <p class="make-ul-text" ms-if="el.processAttachmentType == '7'">X展架</p>
                            <p class="make-ul-text" ms-if="el.processAttachmentType == '8'">台卡卡心</p>
                            <p class="make-ul-text" ms-if="el.processAttachmentType == '9'">易拉宝</p>
                            <p class="make-ul-text" ms-if="el.processAttachmentType == '10'">三折台卡</p>
                            <p class="make-ul-text" ms-if="el.processAttachmentType == '11'">海报</p>
                            <p class="make-ul-text" ms-if="el.processAttachmentType == '13'">桌贴</p>
                        </li>
                    </ul>

                </div>
            </div>
            <div class="operation">
                <p ms-if="promise && el.onlineProcessStatus == '18' && !el.fesMaterialRequests.length">
                <span>
                <button class="simple-btn"
                        ms-click="step7Input(0,'add')"><i class="iconfont icon-blue f26">&#xe60f;</i> 设计需求单</button>
                </span>
                </p>
                <p ms-if="promise && (el.onlineProcessStatus == '18' || el.onlineProcessStatus == '19')">
                <span ms-widget="uploader,$,$step7AttachUploaderOpts">
                <button class="simple-btn"
                        ><i class="iconfont icon-blue f26">&#xe611;</i> 设计素材</button>
                </span>
                </p>
                <p ms-if="promise && el.onlineProcessStatus == '19'">
                <span>
                <button class="simple-btn"
                        ms-click="stepEnd(el.id,'14','你确定物料设计及素材都已经提供了吗？')"><i class="iconfont icon-blue f26">&#xe611;</i> 确认需求</button>
                </span>
                </p>
                <p ms-if="promise && el.onlineProcessStatus == '02'">
                <span>
                <button class="simple-btn"
