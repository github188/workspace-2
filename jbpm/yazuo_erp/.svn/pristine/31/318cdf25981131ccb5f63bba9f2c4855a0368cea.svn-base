<a class="ui-pagination-first-nav"
   ms-if="firstPage!==1"
   ms-class-state-disabled="currentPage == 1"
   ms-title="getTitle('first')"
   ms-click="jumpPage($event,'first')"
   ms-hover="state-hover">首页</a><a class="ui-pagination-prev-nav"
                                   ms-if="firstPage!==1"
                                   ms-class-state-disabled="currentPage == 1"
                                   ms-title="getTitle('prev')"
                                   ms-click="jumpPage($event,'prev')"
                                   ms-hover="state-hover"
        ><i class="iconfont-arrow">&#xf0006;</i>&nbsp;{{prevText}}</a><a class='ui-pagination-ellipsis'
                                                                         ms-if="firstPage!==1 && maxPage > showPages"
        >{{ellipseText}}</a><a class="ui-pagination-show-nav"
                               ms-repeat="pages"
                               ms-href="getHref(el)"
                               ms-title="getTitle(el)"
                               ms-click="jumpPage($event,el)"
                               ms-class-state-active="el == currentPage"
                               ms-hover="state-hover"
        >{{el}}</a><a class="ui-pagination-ellipsis"
                      ms-if="lastPage!==maxPage && maxPage > showPages"
        >{{ellipseText}}</a><a class="ui-pagination-next-nav"
                               ms-if="lastPage!==maxPage"
                               ms-class-state-disabled="currentPage >= maxPage"
                               ms-title="getTitle('next')"
                               ms-click="jumpPage($event,'next')"
                               ms-hover="state-hover"
        >{{nextText}}&nbsp;<i class="iconfont-arrow">&#xe632;</i></a><a class="ui-pagination-last-nav"
                                                                        ms-if="lastPage!==maxPage"
                                                                        ms-class-state-disabled="currentPage >= maxPage"
                                                                        ms-title="getTitle('last')"
                                                                        ms-click="jumpPage($event,'last')"
                                                                        ms-hover="state-hover">末页</a>
