
<div class="page-tbar">
    <ul class="page-nav-crumbs">
        <li class="nav-item" ms-repeat="navCrumbs">
            <a ms-href="el.href" class="nav-l" ms-if="el.href">{{el.text}}</a>
            <span class="nav-text" ms-if="!el.href">{{el.text}}</span>
            <span class="separater" ms-if="el.href">&nbsp;>&nbsp;</span>
        </li>
    </ul>
</div>
<div class="page-bd page-bd-form page-padding">
    <!--client-header-->
    <div class="client-head">
        <!--client-left-->
        <div class="client-head-left">
            <div class="client-head-left-logo"><img src="#"></div>
            <div class="client-head-left-text pt20">
                <h2>{{merchantData.merchantInfo.merchant_name}}</h2>
                <ul>
                    <li>启动日期：{{merchantData.merchantInfo.begin_time|date("yyyy-MM-dd")}}</li>
                    <li>计划上线：{{merchantData.merchantInfo.plan_online_time|date("yyyy-MM-dd")}}</li>
                </ul>
            </div>
        </div>
        <!--client-left-end-->
        <!--client-right-->
        <div class="client_head_right">
            <div class="fn-left pt25">
                <button class="simple-btn mr5" ms-click="localHref('#/frontend/home/mycommunication/')"><i class="iconfont icon-blue f26">&#xe612; </i>通讯录</button>
                <button ms-click="openAdd" class="simple-btn mr5"><i class="iconfont icon-blue f26">&#xe60f; </i>添加问题</button>
                <button ms-click="createActive" class="simple-btn mr5"><i class="iconfont icon-blue f26">&#xe611; </i>活动申请</button>
                <button class="simple-btn mr5"><i class="iconfont icon-blue f26">&#xe614; </i>发送月报</button>
            </div>
        </div>
        <!--client-right-end-->
    </div>
    <!--client-header-end-->
    <!--client-data-->
    <div class="client-top fn-clear mb20">
        <ul class="fn-left client-info-left fn-clear">
            <li>销售负责人：{{merchantData.merchantInfo.sale_user_name}}</li>
            <li>业态：{{merchantData.merchantInfo.format_name}}</li>
            <li>前端负责人：{{merchantData.merchantInfo.front_user_name}}</li>
            <li>门店：{{merchantData.merchantInfo.store_number}}家</li>
            <li>人均：{{merchantData.merchantInfo.person_avg}}元</li>
            <li>桌均：{{merchantData.merchantInfo.desk_avg}}元</li>
        </ul>
        <ul class="fn-right client-info-right">
            <li>开通的产品</li>
            <li>
                <p ms-repeat="merchantData.productList">{{el}}</p>
            </li>
        </ul>
    </div>
    <!--client-data-end-->
    <!--list-->
    <div class="client-step" >
        <!--style-->
        <div class="status-box status-ready">
            <button class="number">1</button>
            <div class="content f14">
                <h2 class="mb5">设备配送</h2>
                <div class="type-box">
                    <ul>
                        <li>
                            <p class="left-title">投诉时间：</p>
                            <p class="left-con">{{el.complaint_time | date("yyyy-MM-dd HH:mm")}}</p>
                        </li>
                        <li>
                            <p class="left-title">投诉类型：</p>
                            <p class="left-con">{{el.customer_complaint_type_name}}</p>
                        </li>
                        <li>
                            <p class="left-title">投诉标题：</p>
                            <p class="left-con">{{el.complaint_title}}</p>
                        </li>
                        <li>
                            <p class="left-title">投诉内容：</p>
                            <p class="left-con">{{el.complaint_content}}</p>
                        </li>
                        <li>
                            <p class="left-title">投诉录音：</p>
                            <p class="left-con"><a ms-href="{{erp.BASE_PATH}}{{el.customerComplaintFilePath}}/{{el.sysAttachment.attachmentName}}">点击下载试听</a></p>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="operation" >
                <button class="simple-btn" ms-click="disposeOpen(el.id)"><i class="iconfont icon-blue f26">&#xe610;</i> 投诉处理确认</button>
            </div>
        </div>
        <!--style-end-->

    </div>
    <!--list-end-->
</div>



<!-- 添加问题 start -->
<div class="add-edit-course-dialog ui-hidden"
     ms-widget="dialog,$,$addQuestionDialogOpts">
    <form class="add-edit-form" ms-widget="form,$,$addQuestionFormOpts"
          action="javascript:;">
        <div class="f-body">
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>商户名称：</label>
                <div class="ff-value">
                    <div class="ff-value-box customer-title" >
                        <p class="merchantName">{{merchantData.merchantInfo.merchant_name}}</p>
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>问题标题：</label>
                <div class="ff-value">
                    <div class="ff-value-box">
                        <input type="text" class="input-text time-limit w310 title" name="title" ms-value="title" />
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>问题类型：</label>
                <div class="ff-value">
                    <div class="ff-value-box">
                        <span class="schedule" ms-widget="select,$,$scheduleOpts"></span>
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>问题描述：</label>
                <div class="ff-value">
                    <div class="ff-value-box">
                        <textarea class="input-text w310 description" name="description" ms-text="description"></textarea>
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
        </div>
    </form>
</div>
<!-- 添加问题 end -->
<!-- 创建营销活动申请 start -->
<div class="add-edit-course-dialog ui-hidden"
     ms-widget="dialog,$,$createActiveDialogOpts">
    <form class="add-edit-form" ms-widget="form,$,$createActiveFormOpts"
          action="javascript:;">
        <div class="f-body">
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>商户名称：</label>
                <div class="ff-value">
                    <div class="ff-value-box customer-title">
                        <p class="merchantName">{{merchantData.merchantInfo.merchant_name}}</p>
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>活动标题：</label>
                <div class="ff-value">
                    <div class="ff-value-box">
                        <input type="text" class="input-text time-limit w310 activityTitle" name="activityTitle" ms-value="activityTitle" />
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>活动类型：</label>
                <div class="ff-value">
                    <div class="ff-value-box">
                        <span class="schedule" ms-widget="select,$,$activeOpts"></span>
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left"><span class="icon-star">*</span>活动说明：</label>
                <div class="ff-value">
                    <div class="ff-value-box">
                        <textarea class="input-text w310 description" name="description" ms-text="description"></textarea>
                    </div>
                    <div class="ff-value-tip"></div>
                </div>

            </div>
            <div class="f-field fn-clear">
                <label class="ff-label fn-left">上传活动表：</label>
                <div class="ff-value">
                    <button class="simple-btn" ms-widget="uploader,$,$activeUploaderOpts">选择文件</button>

                </div>

            </div>
        </div>
    </form>
</div>
<!-- 创建营销活动申请 end -->