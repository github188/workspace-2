package com.yazuo.superwifi.login.vo;

import java.util.Date;


public class Identifyinginfo
{
    /**
     * 主键、存放mac码
     */
    private String id;

    /**
     * 门店ID
     */
    private Integer merchantId;

    /**
     * 品牌ID
     */
    private Integer brandId;

    /**
     * 验证码
     */
    private String identifyingCode;

    /**
     * 通过验证时间
     */
    private Date passTime;

    /**
     * 手机号
     */
    private String mobileNumber;

    /**
     * 状态0删除1未使用2已使用3已过期
     */
    private Integer status;

    /**
     * 插入时间
     */
    private Date insertTime;

    /**
     * 调用来源 1 登录调用 2取回密码调用
     */
    private Integer flag;

    /**
     * 状态:0删除,1未使用,2已使用,3已过期
     */
    public static final Integer STATUS_DETELE = 0;

    /**
     * 状态:0删除,1未使用,2已使用,3已过期
     */
    public static final Integer STATUS_NOUSE = 1;

    /**
     * 状态:0删除,1未使用,2已使用,3已过期
     */
    public static final Integer STATUS_USED = 2;

    /**
     * 状态:0删除,1未使用,2已使用,3已过期
     */
    public static final Integer STATUS_OVERDUE = 3;

    /**
     * 会员登录 2用户取回密码调用
     */
    public static final Integer FLAG_MEMBERLOGIN = 1;

    /**
     * 2用户取回密码调用
     */
    public static final Integer FLAG_USERRESETPASSWORD = 2;

    public String getId()
    {
        return id;
    }

    public void setId(String id)
    {
        this.id = id;
    }

    public Integer getMerchantId()
    {
        return merchantId;
    }

    public void setMerchantId(Integer merchantId)
    {
        this.merchantId = merchantId;
    }

    public Integer getBrandId()
    {
        return brandId;
    }

    public void setBrandId(Integer brandId)
    {
        this.brandId = brandId;
    }

    public String getIdentifyingCode()
    {
        return identifyingCode;
    }

    public void setIdentifyingCode(String identifyingCode)
    {
        this.identifyingCode = identifyingCode;
    }


    /**
     * @return Returns the passTime.
     */
    public Date getPassTime()
    {
        return passTime;
    }

    /**
     * @param passTime The passTime to set.
     */
    public void setPassTime(Date passTime)
    {
        this.passTime = passTime;
    }

    public String getMobileNumber()
    {
        return mobileNumber;
    }

    public void setMobileNumber(String mobileNumber)
    {
        this.mobileNumber = mobileNumber;
    }

    public Integer getStatus()
    {
        return status;
    }

    public void setStatus(Integer status)
    {
        this.status = status;
    }

    /**
     * @return Returns the insertTime.
     */
    public Date getInsertTime()
    {
        return insertTime;
    }

    /**
     * @param insertTime The insertTime to set.
     */
    public void setInsertTime(Date insertTime)
    {
        this.insertTime = insertTime;
    }

    public Integer getFlag()
    {
        return flag;
    }

    public void setFlag(Integer flag)
    {
        this.flag = flag;
    }
}
