/**
 * 框架页面样式
 */
.app{
    /*max-width: 1440px;*/
    min-width: 1280px;
    margin:0 auto;
}
.app-inner-l{
    width:230px;
    border-right:1px solid #e2e2e2;
}
.app-inner-r{
    margin-left:231px;
    overflow: hidden; /* 不明间距产生 */
}
.app-hd .page-title{
    height:50px;
    line-height: 50px;
}
.app-hd .logo-l {
    height:49px;
    line-height: 49px;
    border-bottom: 1px solid #6e9fd5;
    display: block;
    /*background: #6499d3 url(image/logo.png) no-repeat center center;
    text-indent: -10000px;*/
    background: #6499d3;
    font-size: 26px;
    color: #ffffff;
    text-align: center;
    text-decoration: none;
    text-shadow: 2px 2px 4px #666666;
}
.app-hd .page-title{
    background: #76a9e0;
    text-align: center;
    color:#ffffff;
    font-size: 16px;
    font-weight: bold;
}

.app-bd{
    position: relative;
}
.app-bd .app-inner-l {
    box-shadow: 2px 2px 2px #ededed;
}
.app-bd .app-inner-r{
    background: #ffffff;
}
.app-bd .page-mask{
    display: none;
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index: 10;
}
.app-bd .app-inner {
    background: #e4e7eb;
}
@-webkit-keyframes navmove {
    from {
        text-indent: 0;
    }
    to {
        text-indent: -14px;
    }
}
@-moz-keyframes navmove {
    from {
        text-indent: 0;
    }
    to {
        text-indent: -14px;
    }
}
.app-bd .app-inner-l .page-nav-visible-h {
    display: block;
    position:fixed;
    left: 230px;
    top: 50%;
    width: 40px;
    height: 40px;
    margin-top: -20px;
    color: #66ccb5;
    font-size: 40px;
    line-height: 1em;
    text-align: center;
    z-index: 10;
    text-indent: -14px;
    /*-webkit-animation: navmove ease 1s infinite alternate;
    -moz-animation: navmove ease 1s infinite alternate;*/
    cursor: pointer;
    overflow: hidden;
}
.app-bd .app-inner-l .page-nav-visible-h:hover {
    width: 42px;
    height: 90px;
    line-height: 90px;
    margin-top: -45px;
    text-indent: 0;
    animation-play-state:paused;
    -webkit-animation-play-state:paused; /* Safari 和 Chrome */
    border-radius: 0;
    background: #66ccb5;
    color: #ffffff;
}
.app-bd .app-inner-l .page-nav-wrapper {
    background: #eff3f6;
}
.app-bd .app-inner-l .page-nav-1 .nav-title a{
    display: block;
    height: 38px;
    line-height: 38px;
    border-top: 2px solid #66ccb5;
    background: #e4e7eb;
    color: #393939;
    font-size: 14px;
    padding-left: 18px;
    text-decoration: none;
}
.app-bd .app-inner-l .main-nav-list .icon-toggle {

}
.app-bd .app-inner-l .page-nav-2 .nav-title a {
    border-top: none;
    background: transparent;
    display: block;
    height: 34px;
    line-height: 34px;
    padding-left: 40px;
    color: #393939;
    font-size: 14px;
    text-decoration: none;
    font-family: \65B0\5B8B\4F53, \5b8b\4f53;
}
.app-bd .app-inner-l .page-nav-2 .nav-title a.state-hover,
.app-bd .app-inner-l .page-nav-2 .nav-title a.page-nav-state-current {
    background: #66ccb5;
    font-weight: bold;
    color: #ffffff;
}

.page {
    display: none;
}
.page-state-active {
    display: block;
}
