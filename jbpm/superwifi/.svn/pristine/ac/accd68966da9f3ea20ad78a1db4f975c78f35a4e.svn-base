package com.yazuo.superwifi.device.vo;


public class DeviceSSID
{
    /**
     * 主键
     */
    private String id;
    
    private String ssid;
    
    private Boolean isMain;
    
    private Integer ssidBelonging;
    
    private String password;
    
    private Boolean isHavaPassword;
    
    /**
     * ssid归属-会员
     */
    public static final Integer SSIDBELONGING_MEMBER = 1;

    /**
     * ssid归属-店员
     */
    public static final Integer SSIDBELONGING_STAFF = 2;

    /**
     * ssid归属-其他
     */
    public static final Integer SSIDBELONGING_OTHER = 3;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getSsid()
    {
        return ssid;
    }

    public void setSsid(String ssid)
    {
        this.ssid = ssid;
    }

    public Boolean getIsMain() {
		return isMain;
	}

	public void setIsMain(Boolean isMain) {
		this.isMain = isMain;
	}

    /**
     * @return Returns the ssidBelonging.
     */
    public Integer getSsidBelonging()
    {
        return ssidBelonging;
    }

    /**
     * @param ssidBelonging The ssidBelonging to set.
     */
    public void setSsidBelonging(Integer ssidBelonging)
    {
        this.ssidBelonging = ssidBelonging;
    }

    public String getPassword()
    {
        return password;
    }

    public void setPassword(String password)
    {
        this.password = password;
    }

    public Boolean getIsHavaPassword()
    {
        return isHavaPassword;
    }

    public void setIsHavaPassword(Boolean isHavaPassword)
    {
        this.isHavaPassword = isHavaPassword;
    }

    
}
