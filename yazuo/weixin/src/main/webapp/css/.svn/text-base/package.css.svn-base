@charset "utf-8";

* {margin:0;padding:0;}
body {font-family:"Microsoft Yahei";font-size:14px; background:#f5f5f5; color:#393939;}
a{text-decoration:none;color:#fff;font-size:14px;}
ul, ol, li, dl, dt, dd {list-style:none;}
a img {border:0px;}


/*列表*/
.packageItem{ padding:5px 8px; background:#fff; box-shadow:0 0 5px #999; line-height:26px; margin-bottom:15px;}
.packageTitle{ font-size:16px; color:#F3A019; line-height:34px; font-weight:normal;}
.pImg{width:280px; padding:3px; background:#fff; border:#ccc solid 1px; border-radius:3px; margin:0px auto;display:block;}
.packageInfo label{ color:#646464;}