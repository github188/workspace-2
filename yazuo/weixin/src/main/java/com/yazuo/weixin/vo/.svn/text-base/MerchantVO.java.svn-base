package com.yazuo.weixin.vo;

public class MerchantVO {
	private Integer merchantId;
	private String merchantName;
	private String brand;
	private String merchantNo;
	private Boolean isFaceShop;
	private String adColumn;
	private String promptBar;
	private Integer parent;
	private Integer status;
	private String brandShortPinyin;
	private Integer brandId;
	private String thirdpartyMerchantNo;
	public Integer getMerchantId() {
		return merchantId;
	}
	public void setMerchantId(Integer merchantId) {
		this.merchantId = merchantId;
	}
	public String getMerchantName() {
		return merchantName;
	}
	public void setMerchantName(String merchantName) {
		this.merchantName = merchantName;
	}
	public String getBrand() {
		return brand;
	}
	public void setBrand(String brand) {
		this.brand = brand;
	}
	public String getMerchantNo() {
		return merchantNo;
	}
	public void setMerchantNo(String merchantNo) {
		this.merchantNo = merchantNo;
	}
	public Boolean getIsFaceShop() {
		return isFaceShop;
	}
	public void setIsFaceShop(Boolean isFaceShop) {
		this.isFaceShop = isFaceShop;
	}
	public String getAdColumn() {
		return adColumn;
	}
	public void setAdColumn(String adColumn) {
		this.adColumn = adColumn;
	}
	public String getPromptBar() {
		return promptBar;
	}
	public void setPromptBar(String promptBar) {
		this.promptBar = promptBar;
	}
	public Integer getParent() {
		return parent;
	}
	public void setParent(Integer parent) {
		this.parent = parent;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getBrandShortPinyin() {
		return brandShortPinyin;
	}
	public void setBrandShortPinyin(String brandShortPinyin) {
		this.brandShortPinyin = brandShortPinyin;
	}
	public Integer getBrandId() {
		return brandId;
	}
	public void setBrandId(Integer brandId) {
		this.brandId = brandId;
	}
	public String getThirdpartyMerchantNo() {
		return thirdpartyMerchantNo;
	}
	public void setThirdpartyMerchantNo(String thirdpartyMerchantNo) {
		this.thirdpartyMerchantNo = thirdpartyMerchantNo;
	}
	
}
