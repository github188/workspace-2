package com.yazuo.weixin.vo;

/**
 * @ClassName: CouponDescribeVO
 * @Description: 优惠券描述bean
 */
public class CouponDescribeVO {

	private String description;// 描述信息
	private String coupon_name;// 优惠券名称
	private Integer brand_id;// 集团id
	private Integer flag;// 标识

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getCoupon_name() {
		return coupon_name;
	}

	public void setCoupon_name(String coupon_name) {
		this.coupon_name = coupon_name;
	}

	public Integer getBrand_id() {
		return brand_id;
	}

	public void setBrand_id(Integer brand_id) {
		this.brand_id = brand_id;
	}

	public Integer getFlag() {
		return flag;
	}

	public void setFlag(Integer flag) {
		this.flag = flag;
	}

	@Override
	public String toString() {
		return "CouponDescribeVO [description=" + description
				+ ", coupon_name=" + coupon_name + ", brand_id=" + brand_id
				+ ", flag=" + flag + "]";
	}

}
