.page-databank-knowledgebank .page-bd {
    padding: 10px;
}
.page-databank-knowledgebank .page-content {
    width: 100%;
    padding-bottom: 10px;
    overflow: hidden;
}
.page-databank-knowledgebank .page-left {
    position: relative;
    overflow: hidden;
    float: left;
    width: 20%;
    border:1px solid #CDE2F9;
    background: #fff;
}
.page-databank-knowledgebank .page-left-con {
    padding: 10px 0;
}
.page-databank-knowledgebank .page-left-search {
    position: relative;
    overflow: hidden;
    border: 1px solid #2cc78f;
    margin: 0 10px 10px 10px;
}
.page-databank-knowledgebank .button-style {
    position: absolute;
    right: 0;
    top: 0;
    border: none;
    cursor: pointer;
    background: none;
    width: 30px;
    height: 28px;
    text-align: center;
}
.page-databank-knowledgebank .page-left-search input {
    border: none;
    width: 80%;
}
.page-databank-knowledgebank .page-left .class-tree-padding {
    padding: 0 20px 0 10px;
    overflow: hidden;
    max-height: 535px;
    overflow-y: auto;
}
.page-databank-knowledgebank .page-left .create-knowledge-tree {
    width: 100%;
    overflow: hidden;
}



.page-databank-knowledgebank .page-right {
    float: right;
    width: 79%;
}
.page-databank-knowledgebank .page-left div.dragEle {
    position: absolute;
    top: 0;
    right: 0;
    width: 3px;
    height: 100%;
    background: #CDE2F9;
    cursor: ew-resize;
}
.page-databank-knowledgebank .page-grid {
    border: 1px solid #CDE2F9;
    margin-top: 20px;
    padding: 10px;
    background: #fff;
}
.page-databank-knowledgebank .page-grid-head {
    margin-bottom: 20px;
    font-size: 14px;
    color: #666;
}
.page-databank-knowledgebank .page-grid-head .input-text {
    width: 12%;
}
.page-databank-knowledgebank .page-grid-body {
    border: 1px solid #CDE2F9;
    background: #fff;
}
.page-databank-knowledgebank .grid-title-width {
    width: 200px;
    overflow: hidden;
    text-align: left;
    text-overflow: ellipsis;
    white-space:nowrap;
    word-break:keep-all;
}
.page-databank-knowledgebank .grid-con-width {
    width: 100px;
    overflow: hidden;
    text-align: left;
    text-overflow: ellipsis;
    white-space:nowrap;
    word-break:keep-all;
}
.page-databank-knowledgebank .grid-class-width {
    width: 100px;
    overflow: hidden;
    text-align: left;
    text-overflow: ellipsis;
    white-space:nowrap;
    word-break:keep-all;
}
.page-databank-knowledgebank .grid-select-width {
    width: 80px;
}












