}
.page-backend-customerservice-waiting .status-bging .number {
    background: #0099CC;
}
.page-backend-customerservice-waiting .status-bging .content h2 {
    color:#0099CC;
}
.page-backend-customerservice-waiting .status-bging .status-style {
    background:#fff;
    border:1px solid #0099CC;
}
/*已经结束*/
.page-backend-customerservice-waiting .status-end {
    border:1px solid #2CC78F;
    background: #eafff1;
}
.page-backend-customerservice-waiting .status-end .number {
    background: #2CC78F;
}
.page-backend-customerservice-waiting .status-end .content h2 {
    color:#2CC78F;
}
.page-backend-customerservice-waiting .status-end .status-style {
    background:#fff;
    border:1px solid #2CC78F;
}
/*已超时*/
.page-backend-customerservice-waiting .status-past {
    border:1px solid #FF7800;
