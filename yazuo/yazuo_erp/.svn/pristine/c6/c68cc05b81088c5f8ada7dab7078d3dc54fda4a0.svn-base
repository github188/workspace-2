
.page-backend-customerservice-waiting {
    color:#777;
}
.page-backend-customerservice-waiting .page-padding{
    padding:20px 15px;
}
.page-backend-customerservice-waiting .client-head{
    width:100%;
    overflow:hidden;
    margin-bottom: 20px;
}
.page-backend-customerservice-waiting .client-head-left{
    float:left;
    display:inline;
}
.page-backend-customerservice-waiting .client-head-left-logo{
    float:left;display:inline;
    width:100px;
    height:100px;
}
.page-backend-customerservice-waiting .client-head-left-logo img{
    width:100px;
    height:100px;
    border:1px solid #ccc;
}
.page-backend-customerservice-waiting .client-head-left-text{
    float:left;display:inline;
    margin-left:30px;
    width:350px;
}
.page-backend-customerservice-waiting .client_head_right{
    float:right;
    display:inline;
}
.page-backend-customerservice-waiting .client-head-left-text h2 {
    font-size:24px;
    line-height:30px;
}
.page-backend-customerservice-waiting .client-head-left-text ul {
    width:100%;
    overflow:hidden;
}
.page-backend-customerservice-waiting .client-head-left-text ul li {
    float:left;
    display: inline;
    width:170px;
    line-height:23px;
    margin:10px 0;
}
.page-backend-customerservice-waiting .mail {
    background: #FDF4DA;
    padding:10px;
}
.page-backend-customerservice-waiting .client-top {
    border:1px solid #DDE3EA;
    overflow:hidden;
}
.page-backend-customerservice-waiting .client-info-left {
    padding:15px;
    width:650px;
}
.page-backend-customerservice-waiting .client-info-right {
    padding:15px;
    width:300px;
    padding-bottom:200px;
    margin-bottom:-200px;
    border-left:1px solid #DDE3EA;
}
.page-backend-customerservice-waiting .client-info-left li {
    float:left;
    display:inline;
    font-size:16px;
    width:300px;
    margin:8px 5px;
}
.page-backend-customerservice-waiting .client-info-right li {
    font-size:16px;
    margin-top:5px;
    width:100%;
    overflow:hidden;
}
.page-backend-customerservice-waiting .client-info-right li p {
    float:left;
    display:inline;
    width:130px;
    margin:2px 0;
}
.page-backend-customerservice-waiting .client-info-right li p a {
    font-size:14px;
}
.page-backend-customerservice-waiting .swfupload {
    left:0;
}


.page-backend-customerservice-waiting .client-step {
    margin-top :15px;
}
.page-backend-customerservice-waiting .status-box {
    position: relative;
    width:100%;
    overflow: hidden;
    background:#fff;
    margin-bottom:15px;
    border:1px solid #E1E1E1;
}
.page-backend-customerservice-waiting .status-box .number {
    width:55px;
    position:absolute;
    left:0px;
    top:0px;
    font-size:28px;
    height:100%;
    border:none;
    background:#D5D2CD;
    color:#fff;

}
.page-backend-customerservice-waiting .status-box .content {
    position:relative;
    margin-left:70px;
    margin-right:345px;
    padding:10px 0;
}
.page-backend-customerservice-waiting .linewaiting-input {
    border: 1px solid #ccc;
    display: inline-block;
    padding:2px 10px;
    margin: 0 15px 10px 0;
    cursor:pointer;

}
.page-backend-customerservice-waiting .status-box .operation {
    width:330px;
    position: absolute;
    right:0px;
    top:50%;
    height:100%;
    margin-top:-17px;
    text-align:right;
}
.page-backend-customerservice-waiting .status-box .operation p {
    display:inline-block;

}
.page-backend-customerservice-waiting .status-box .content h2 {
    font-size:20px;
}
.page-backend-customerservice-waiting .type-box {
    padding: 5px;
}
.page-backend-customerservice-waiting .type-box li {
    padding:5px 0;
    width: 100%;
    overflow: hidden;
    background: #F4F3F2;
    margin:3px 0;
    position: relative;
}
.page-backend-customerservice-waiting .type-box li b{
    position: absolute;
    right:3px;
    top:3px;
    cursor: pointer;
}
.page-backend-customerservice-waiting .type-box li .left-title {
    float: left;
    display: inline;
    width:120px;
    text-align: right;
    line-height: 20px;
}
.page-backend-customerservice-waiting .type-box li .left-con {
    float: left;
    display: inline;
    width:490px;
    padding-left:10px;
    text-align: left;
    line-height: 20px;
    position: relative;
}

.merchantName {
    font-size: 14px;
}

.page-backend-customerservice-waiting-dialog .page-backend-customerservice-waiting-box {
    border-bottom: 1px dashed #ccc;
    padding:10px;
}
.page-backend-customerservice-waiting-dialog .page-backend-customerservice-waiting-box-title {
    font-size: 14px;
    color: #777;
    padding: 8px 0;
}
.page-backend-customerservice-waiting-dialog .page-backend-customerservice-waiting-box-con {
    font-size: 14px;
    color: #777;
    padding: 8px 0;
    padding-left: 32px;
}
.page-backend-customerservice-waiting-dialog .page-backend-customerservice-waiting-box-con label {
    margin-right: 20px;
}
.page-backend-customerservice-waiting-box-con textarea {
    width: 470px;
    height:60px;
}
.page-backend-customerservice-waiting-box-con p {
    padding: 6px 0;
}
.page-backend-customerservice-waiting-dialog .ui-dialog-content {
    padding: 20px 0 20px 20px;
}

.waiting-list {
    border :1px solid #ccc;
    background: #eee;
    position: relative;
    margin-bottom: 15px;
    padding:10px;
    position: relative;
}
.con-pdtop {
    padding-top:5px;
}
.con-pdtop label {
    margin-right: 15px;
    color: #333;
}
.waiting-card .ui-dialog-content {
    padding: 20px 0 20px 20px;
}
.waiting-list .remove-list {
    position: absolute;
    top: 3px;
    right:3px;
    cursor: pointer;
}


/*未开始*/
.page-backend-customerservice-waiting .status-ready {
    border:1px solid #E1E1E1;
}
.page-backend-customerservice-waiting .status-ready .number {
    background:#E1E1E1;
}
.page-backend-customerservice-waiting .status-ready .status-style {
    background:#fff;
    border:1px solid #E1E1E1;
}
/*已经开始*/
.page-backend-customerservice-waiting .status-bging {
    border:1px solid #0099CC;
