}
.page-backend-customerservice-waiting .status-past .content h2 {
    color:#FF7800;
}
.page-backend-customerservice-waiting .status-past .status-style {
    background:#fff;
    border:1px solid #FF7800;
}
.page-backend-customerservice-waiting .content-list {
    width:100%;
    overflow: hidden;
    margin:5px 0;
}

.page-backend-customerservice-waiting .page-button {
    width: 180px;
    overflow: hidden;
    margin: 0 5px 0 0;
    height: 25px;
    position: relative;
    float: left;
    display: inline-block;
    cursor: pointer;
}
.page-backend-customerservice-waiting .page-button .text {
    position: absolute;
    left: 0;
    top: 0;
    width: 120px;
    padding: 0 5px;
    text-align: center;
    overflow: hidden;
    background: #fff;
    line-height: 25px;
    margin: 0;
    color : #005ea7;
}
.page-backend-customerservice-waiting .page-button .choice {
    position: absolute;
    right: 0;
    top: 0;
    overflow: hidden;
    width: 50px;
    height: 25px;
    margin: 0;
    padding: 0;
    background: #fff;
}
/*未开始*/
.page-backend-customerservice-waiting .status-ready .page-button .choice i {
    float: right;
    display: inline-block;
    width: 23px;
    height: 25px;
    border-left:1px solid #e1e1e1;
    text-align: center;
}
/*已经开始*/
.page-backend-customerservice-waiting .status-bging .page-button .choice i {
    float: right;
    display: inline-block;
    width: 23px;
    height: 25px;
    border-left:1px solid #0099cc;
    text-align: center;
}
/*已经结束*/
.page-backend-customerservice-waiting .status-end .page-button .choice i {
    float: right;
    display: inline-block;
    width: 23px;
    height: 25px;
    border-left:1px solid #2cc78f;
    text-align: center;
}
/*已超时*/
.page-backend-customerservice-waiting .status-past .page-button .choice i {
    float: right;
    display: inline-block;
    width: 23px;
    height: 25px;
    border-left:1px solid #FF7800;
    text-align: center;
}
