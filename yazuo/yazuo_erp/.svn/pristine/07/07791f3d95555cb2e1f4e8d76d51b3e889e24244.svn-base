package com.yazuo.erp.base;

/**
 * @Description 
 * @author song
 * @date 2014-9-25 上午10:55:41
 */
public class EmailVO {
	
	private String sendAddress = null; //发件人地址
	private String subject; //邮件主题
	private String context; //邮件正文
	private String[] to;//收件人
	private Integer merchantId;//商户ID， 发送月报的时候用到
	private Integer mailTemplateId;//邮件模板ID
	private String smsContent; //短信正文
	
	public String getSmsContent() {
		return smsContent;
	}
	public void setSmsContent(String smsContent) {
		this.smsContent = smsContent;
	}
	public Integer getMailTemplateId() {
		return mailTemplateId;
	}
	public void setMailTemplateId(Integer mailTemplateId) {
		this.mailTemplateId = mailTemplateId;
	}
	public Integer getMerchantId() {
		return merchantId;
	}
	public void setMerchantId(Integer merchantId) {
		this.merchantId = merchantId;
	}
	public String getSendAddress() {
		return sendAddress;
	}
	public void setSendAddress(String sendAddress) {
		this.sendAddress = sendAddress;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public String getContext() {
		return context;
	}
	public void setContext(String context) {
		this.context = context;
	}
	public String[] getTo() {
		return to;
	}
	public void setTo(String[] to) {
		this.to = to;
	}
	
}
